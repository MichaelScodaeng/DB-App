<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryBlue">#007ACC</Color>
    <Color x:Key="LightBlue">#4EB8FF</Color>
    <Color x:Key="DarkBlue">#005BA1</Color>
    <Color x:Key="AccentBlue">#00BFFF</Color>
    <Color x:Key="AccentGrey">#808086</Color>
    <Color x:Key="VeryLightBlue">#cadeef</Color>
    <SolidColorBrush x:Key="PrimaryColorBrush" Color="{StaticResource PrimaryBlue}" />
    <SolidColorBrush x:Key="LightColorBrush" Color="{StaticResource LightBlue}" />
    <SolidColorBrush x:Key="DarkColorBrush" Color="{StaticResource DarkBlue}" />
    <SolidColorBrush x:Key="AccentBlueColorBrush" Color="{StaticResource AccentBlue}" />
    <SolidColorBrush x:Key="AccentGreyColorBrush" Color="{StaticResource AccentGrey}" />
    <SolidColorBrush x:Key="VeryLightColorBrush" Color="{StaticResource VeryLightBlue}" />
</ResourceDictionary>